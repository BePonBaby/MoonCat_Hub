        ["Great Tree"] = CFrame.new(2000, 17, -5000),
        ["Floating Turtle"] = CFrame.new(-12550, 332, -7870),
        ["Castle on the Sea"] = CFrame.new(-5000, 17, 1000),
        ["Haunted Castle"] = CFrame.new(-9500, 17, 5000),
        ["Sea of Treats"] = CFrame.new(0, 17, -10000),
        ["Tiki Outpost"] = CFrame.new(-16250, 100, 1000),
        ["Heaven Dimension"] = CFrame.new(0, 1000, 0),
        ["Hell Dimension"] = CFrame.new(0, -1000, 0),
        ["Frozen Dimension"] = CFrame.new(5000, 17, 5000),
        ["Treasure Island"] = CFrame.new(-10000, 17, -5000),
        ["Mirage Island"] = CFrame.new(0, 100, 0)
    }
    teleportTo(locations[selected])
end)

-- 6Ô∏è‚É£ Tab Auto Mastery & Stats
local MasteryTab = Window:NewTab("Mastery & Stats")
local MasterySection = MasteryTab:NewSection("Mastery & Stats Features")
MasterySection:NewToggle("Auto Mastery", "T·ª± ƒë·ªông n√¢ng Mastery", function(state)
    toggles.AutoMastery = state
    playClickSound()
    if state then
        spawn(function()
            while toggles.AutoMastery do
                for _, enemy in pairs(Workspace.Enemies:GetChildren()) do
                    if enemy:FindFirstChild("Humanoid") then
                        teleportTo(enemy.HumanoidRootPart.CFrame)
                        wait(attackSpeed)
                    end
                end
                wait(1)
            end
        end)
    end
end)
MasterySection:NewDropdown("Auto Stats", "Ph√¢n b·ªï Stats", {"Melee", "Defense", "Sword", "Gun", "Fruit"}, function(selected)
    statPoint = selected
    playClickSound()
end)
MasterySection:NewToggle("Enable Auto Stats", "K√≠ch ho·∫°t Auto Stats", function(state)
    toggles.AutoStats = state
    playClickSound()
    if state then
        spawn(function()
            while toggles.AutoStats do
                ReplicatedStorage.Remotes.CommF_:InvokeServer("AddPoint", statPoint, 1)
                wait(1)
            end
        end)
    end
end)

-- 7Ô∏è‚É£ Tab Boss Farm & Auto Chest
local BossTab = Window:NewTab("Boss & Chest")
local BossSection = BossTab:NewSection("Boss & Chest Features")
BossSection:NewToggle("Boss Farm", "T·ª± ƒë·ªông ƒë√°nh boss", function(state)
    toggles.BossFarm = state
    playClickSound()
    if state then
        spawn(function()
            while toggles.BossFarm do
                for _, boss in pairs(Workspace.Enemies:GetChildren()) do
                    if boss.Name:match("Boss") then
                        teleportTo(boss.HumanoidRootPart.CFrame)
                        wait(attackSpeed)
                    end
                end
                wait(2)
            end
        end)
    end
end)
BossSection:NewToggle("Auto Chest", "T·ª± ƒë·ªông nh·∫∑t r∆∞∆°ng", function(state)
    toggles.AutoChest = state
    playClickSound()
    if state then
        spawn(function()
            while toggles.AutoChest do
                for _, chest in pairs(Workspace:GetChildren()) do
                    if chest.Name == "Chest" then
                        teleportTo(chest.CFrame)
                        wait(0.5)
                    end
                end
                wait(1)
            end
        end)
    end
end)

-- 8Ô∏è‚É£ Tab Auto V4 Race
local V4Tab = Window:NewTab("V4 Race")
local V4Section = V4Tab:NewSection("V4 Race Features")
V4Section:NewToggle("Auto V4 Race", "T·ª± ƒë·ªông n√¢ng c·∫•p V4", function(state)
    toggles.AutoV4Race = state
    playClickSound()
    if state then
        spawn(function()
            while toggles.AutoV4Race do
                if game.PlaceId == 2753915549 and isFullMoon() then
                    for _, island in pairs(Workspace:GetChildren()) do
                        if island.Name == "Mirage Island" then
                            teleportTo(island.HighestPoint.CFrame)
                            ReplicatedStorage.Remotes.CommF_:InvokeServer("Awakening", "ResonateMirrorFractal")
                            wait(5)
                            for _, gear in pairs(island:GetChildren()) do
                                if gear.Name == "Blue Gear" then
                                    teleportTo(gear.CFrame)
                                    break
                                end
                            end
                            teleportTo(CFrame.new(28700, 1500, -5000))
                            ReplicatedStorage.Remotes.CommF_:InvokeServer("Awakening", "PullLever")
                            local race = getPlayerRace()
                            if race == "Human" then
                                teleportTo(CFrame.new(28750, 1500, -5100))
                                for _, enemy in pairs(Workspace.Enemies:GetChildren()) do
                                    if enemy.Name == "TrialEnemy" then
                                        teleportTo(enemy.HumanoidRootPart.CFrame)
                                        enemy.Humanoid.Health = 0
                                        wait(attackSpeed)
                                    end
                                end
                            end
                            ReplicatedStorage.Remotes.CommF_:InvokeServer("Awakening", "UnlockV4")
                            break
                        end
                    end
                end
                wait(10)
            end
        end)
    end
end)

-- 9Ô∏è‚É£ Tab Movement
local MovementTab = Window:NewTab("Movement")
local MovementSection = MovementTab:NewSection("Movement Features")
MovementSection:NewToggle("Fly Hack", "Bay t·ª± do", function(state)
    toggles.FlyHack = state
    playClickSound()
    if state then
        spawn(function()
            local bodyVelocity = Instance.new("BodyVelocity")
            bodyVelocity.Velocity = Vector3.new(0, flySpeed, 0)
            bodyVelocity.Parent = LocalPlayer.Character.HumanoidRootPart
            while toggles.FlyHack do
                wait()
            end
            bodyVelocity:Destroy()
        end)
    end
end)
MovementSection:NewToggle("No Clip", "Xuy√™n t∆∞·ªùng", function(state)
    toggles.NoClip = state
    playClickSound()
    if state then
        spawn(function()
            while toggles.NoClip do
                for _, part in pairs(LocalPlayer.Character:GetChildren()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
                wait()
            end
        end)
    end
end)

-- 10Ô∏è‚É£ Tab ESP
local ESPTab = Window:NewTab("ESP")
local ESPSection = ESPTab:NewSection("ESP Features")
ESPSection:NewToggle("ESP", "Hi·ªÉn th·ªã m·ª•c ti√™u", function(state)
    toggles.ESP = state
    playClickSound()
    if state then
        spawn(function()
            while toggles.ESP do
                for _, obj in pairs(Workspace:GetChildren()) do
                    if obj:FindFirstChild("Humanoid") or obj.Name == "Chest" or obj:IsA("Tool") then
                        local esp = Instance.new("BillboardGui", obj)
                        esp.Size = UDim2.new(0, 100, 0, 50)
                        esp.StudsOffset = Vector3.new(0, 3, 0)
                        local text = Instance.new("TextLabel", esp)
                        text.Size = UDim2.new(1, 0, 1, 0)
                        text.Text = obj.Name
                        text.BackgroundTransparency = 1
                        text.TextColor3 = Color3.fromRGB(255, 0, 0)
                    end
                end
                wait(5)
            end
        end)
    end
end)

-- 11Ô∏è‚É£ Tab PvP & Mob
local PvPTab = Window:NewTab("PvP & Mob")
local PvPSection = PvPTab:NewSection("PvP & Mob Features")
PvPSection:NewToggle("Bring Mob", "Gom qu√°i", function(state)
    toggles.BringMob = state
    playClickSound()
    if state then
        spawn(function()
            while toggles.BringMob do
                for _, enemy in pairs(Workspace.Enemies:GetChildren()) do
                    if enemy:FindFirstChild("HumanoidRootPart") then
                        enemy.HumanoidRootPart.CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 5)
                    end
                end
                wait(1)
            end
        end)
    end
end)
PvPSection:NewToggle("Bounty Hunt", "SƒÉn Bounty PvP", function(state)
    toggles.BountyHunt = state
    playClickSound()
    if state then
        spawn(function()
            while toggles.BountyHunt do
                for _, player in pairs(Players:GetPlayers()) do
                    if player ~= LocalPlayer and player.Character then
                        teleportTo(player.Character.HumanoidRootPart.CFrame)
                        wait(attackSpeed)
                    end
                end
                wait(2)
            end
        end)
    end
end)

-- 12Ô∏è‚É£ Tab Auto Weapons
local WeaponTab = Window:NewTab("Weapons")
local WeaponSection = WeaponTab:NewSection("Weapon Features")
WeaponSection:NewToggle("Auto Tushita", "T·ª± ƒë·ªông l·∫•y Tushita", function(state)
    toggles.AutoTushita = state
    playClickSound()
    if state then
        spawn(function()
            teleportTo(CFrame.new(-12550, 332, -7870))
            for _, enemy in pairs(Workspace.Enemies:GetChildren()) do
                if enemy:FindFirstChild("Humanoid") then
                    teleportTo(enemy.HumanoidRootPart.CFrame)
                    enemy.Humanoid.Health = 0
                    wait(attackSpeed)
                end
            end
            teleportTo(CFrame.new(5750, 610, -280))
            wait(1)
            teleportTo(CFrame.new(-10200, 996, -6870))
            for _, boss in pairs(Workspace.Enemies:GetChildren()) do
                if boss.Name == "rip_indra True Form" then
                    teleportTo(boss.HumanoidRootPart.CFrame)
                    boss.Humanoid.Health = 0
                    wait(1)
                    ReplicatedStorage.Remotes.CommF_:InvokeServer("BuyTushita")
                end
            end
        end)
    end
end)
WeaponSection:NewToggle("Auto Yama", "T·ª± ƒë·ªông l·∫•y Yama", function(state)
    toggles.AutoYama = state
    playClickSound()
    if state then
        spawn(function()
            teleportTo(CFrame.new(-12550, 332, -7870))
            for i = 1, 30 do
                for _, elite in pairs(Workspace.Enemies:GetChildren()) do
                    if elite.Name == "Elite Pirate" then
                        teleportTo(elite.HumanoidRootPart.CFrame)
                        elite.Humanoid.Health = 0
                        wait(attackSpeed)
                    end
                end
            end
            teleportTo(CFrame.new(5230, 602, -280))
            ReplicatedStorage.Remotes.CommF_:InvokeServer("PullYama")
            wait(1)
            ReplicatedStorage.Remotes.CommF_:InvokeServer("BuyYama")
        end)
    end
end)
WeaponSection:NewToggle("Auto Shark Anchor", "T·ª± ƒë·ªông l·∫•y Shark Anchor", function(state)
    toggles.AutoSharkAnchor = state
    playClickSound()
    if state then
        spawn(function()
            teleportTo(CFrame.new(-16250, 100, 1000))
            for _, seaBeast in pairs(Workspace.SeaBeasts:GetChildren()) do
                if seaBeast.Name == "Terror Shark" then
                    teleportTo(seaBeast.HumanoidRootPart.CFrame)
                    seaBeast.Humanoid.Health = 0
                    wait(attackSpeed)
                end
            end
            teleportTo(CFrame.new(-16550, 55, 2300))
            ReplicatedStorage.Remotes.CommF_:InvokeServer("BuySharkAnchor")
        end)
    end
end)

-- 13Ô∏è‚É£ Tab Settings
local SettingsTab = Window:NewTab("Settings")
local SettingsSection = SettingsTab:NewSection("UI & Game Settings")
SettingsSection:NewToggle("Dark/Light Mode", "Chuy·ªÉn ch·∫ø ƒë·ªô s√°ng/t·ªëi", function(state)
    toggles.DarkMode = state
    playClickSound()
    setUIMode(state)
end, toggles.DarkMode)
SettingsSection:NewSlider("Attack Speed", "T·ªëc ƒë·ªô ƒë√°nh (gi√¢y)", 1, 0.1, 0.5, function(value)
    attackSpeed = value
    playClickSound()
end)
SettingsSection:NewSlider("Fly Speed", "T·ªëc ƒë·ªô bay", 200, 10, 50, function(value)
    flySpeed = value
    playClickSound()
    if toggles.FlyHack then
        local bodyVelocity = LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyVelocity")
        if bodyVelocity then
            bodyVelocity.Velocity = Vector3.new(0, flySpeed, 0)
        end
    end
end)
SettingsSection:NewSlider("Walk Speed", "T·ªëc ƒë·ªô ƒëi b·ªô", 100, 16, 16, function(value)
    walkSpeed = value
    playClickSound()
    LocalPlayer.Character.Humanoid.WalkSpeed = walkSpeed
end)
SettingsSection:NewSlider("Jump Power", "ƒê·ªô cao nh·∫£y", 200, 50, 50, function(value)
    jumpPower = value
    playClickSound()
    LocalPlayer.Character.Humanoid.JumpPower = jumpPower
end)
SettingsSection:NewButton("Remove Fog", "X√≥a s∆∞∆°ng m√π", function()
    playClickSound()
    Lighting.FogEnd = 100000
    Lighting.FogStart = 100000
    if Lighting:FindFirstChild("Atmosphere") then
        Lighting.Atmosphere:Destroy()
    end
end)
SettingsSection:NewToggle("Anti-Ban", "Gi·∫£m nguy c∆° ban", function(state)
    toggles.AntiBan = state
    playClickSound()
    if state then wait(5) end -- Gi·∫£ l·∫≠p Anti-Ban
end)

-- Th√¥ng b√°o khi ch·∫°y
Library:Notify("MoonCat Hub ƒë√£ ƒë∆∞·ª£c k√≠ch ho·∫°t!", 5)
